@model List<ResultUserDto>

<div class="team-area pt-160 pb-160 section-bg" data-background="/Vitrin%20Temaları/onlineedu-master/assets/img/gallery/section_bg02.png">
    <div class="container">
        <div class="row justify-content-center">
            <div class="cl-xl-7 col-lg-8 col-md-10">

                <div class="section-tittle section-tittle2 text-center mb-70">
                    <span>Eğitmenlerimiz</span>
                    <h2>Alanında Uzman Eğitim Kadromuz</h2>
                </div>
            </div>
        </div>
        <div class="row">
            @foreach (var Users in Model)
            {
                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="single-team mb-30">
                        <div class="team-img">
                            <img src="~/Vitrin Temaları/onlineedu-master/assets/img/gallery/team1.png" alt="@Users.FirsName @Users.LastName">
                            <!-- Blog Social -->
                            <ul class="team-social">
                                @foreach (var SocialMedias in Users.TeacherSocialMedias)
                                {
                                    <li><a href="@SocialMedias.Url" target="_blank"><i class="@SocialMedias.Icon"></i></a></li>
                                }
                            </ul>
                        </div>
                        <div class="team-caption">
                            <h3><a href="instructor.html">@Users.FirsName @Users.LastName</a></h3>
                            <p>Eğitmen</p>
                        </div>
                    </div>
                </div>
            }
        </div>
  
        <div class="row">
            <div class="col-lg-12">
                <div class="browse-btn2 text-center mt-70">
                    <a href="/Teacher/Index" class="btn white-btn">Tüm Eğitmenleri Gör</a>
                </div>
            </div>
        </div>
    </div>
</div>
